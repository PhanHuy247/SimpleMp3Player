<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="vn.techkid.simplemp3player.Fragment.ArtistCountryFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="6dp">
    <Button
        android:id="@+id/btnartistcountryvietnam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ArtistVietNam"
        android:textColor="@color/black"
        android:background="@drawable/button_playback"
        android:layout_marginTop="8dp"
        />
        <Button
            android:id="@+id/btnartistcountryusuk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ArtistUSUK"
            android:textColor="@color/black"
            android:background="@drawable/button_playback"
            android:layout_marginTop="8dp"
            />
        <Button
            android:id="@+id/btnartistcountrykorea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ArtistKorea"
            android:textColor="@color/black"
            android:background="@drawable/button_playback"
            android:layout_marginTop="8dp"
            />
    </LinearLayout>

</FrameLayout>
